{
  "name": "conncon-react",
  "version": "0.0.0",
  "description": "A project to create the ConnCon.com website in React",
  "scripts": {
    "build": "webpack -p",
    "bundle": "webpack",
    "start": "webpack-dev-server -d --content-base ./public --open chrome",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spawnbroker/ConnCon-React.git"
  },
  "author": "Jim Eiche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Spawnbroker/ConnCon-React/issues"
  },
  "homepage": "https://github.com/Spawnbroker/ConnCon-React#readme",
  "dependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^4.4.1",
    "ts-loader": "^6.2.1",
    "webpack-dev-server": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.7.2",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
